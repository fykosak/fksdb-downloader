{
  "name": "fykosak/nette-fksdb-downloader",
  "type": "library",
  "description": "FKSDB Downloader for nette web applications",
  "require": {
    "php": ">=7.4",
    "fykosak/fksdb-downloader-core": "v1.5.1",
    "nette/utils": "^3.2",
    "nette/caching": "^3.1",
    "ext-dom": "*",
    "ext-soap": "*",
    "netresearch/jsonmapper": "^4.0",
    "ext-json": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0"
  },
  "scripts": {
    "testPSR": "php -e vendor/squizlabs/php_codesniffer/bin/phpcs --standard=PSR12 src/"
  },
  "authors": [
    {
      "name": "FYKOS",
      "email": "webmaster@fykos.cz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fykosak\\NetteFKSDBDownloader\\": "src/"
    }
  }
}
